@{
    ViewData["Title"] = "Home Page";
}
<!--link rel="stylesheet" href="~/css/gallery.css" /-->
<link href="https://fonts.googleapis.com/css?family=Dosis:400,700" rel="stylesheet">
<link rel="stylesheet" href="~/css/cards.css" />
<script src="~/js/discoverScripts.js"></script>
<link rel="stylesheet" href="~/css/loader.css" />


<h2 style="text-align:center "> Welcome to Movie Station !</h2>
<body onload="main()" , id="moviesL">

    <div id="loader"></div>
    <div style="display:none;" id="myDiv" class="animate-bottom">
        <div class="bar">
            <button id="previous" class="button" onClick="previousClick()">  < </button>
            <p id="page">1</p>
            <button id="next" class="button" onclick="nextClick()">  > </button>

            <input type="text" id="toPage" style="width:40px; margin-left:30px">
            <button id="go" class="button" style="width:3%" onclick="jumpPage()">  Go! </button>
        </div>

        <div id="root"> </div>
</div>
</body>


